<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
<link rel='stylesheet' href='/stylesheets/style.css' />
<link rel="stylesheet"
	href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
<link rel="stylesheet"
	href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
<script
	src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
<script
	src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
<script>

var pl = new SOAPClientParameters();
pl.add("name", document.frmDemo.txtName.value);
SOAPClient.invoke("http://localhost:8080/calwebserver/services/cal?wsdl", "HelloTo", pl, true, HelloTo_callBack);

function HelloTo_callBack(r)
{
    alert(r+"Pls check your input");
}
SOAPClient
</script>
<script src="angularjs/cal.js"></script>
</head>
  <body ng-app="cal" ng-controller="cal" class="bg-info">
    <h1><%= title %></h1>
    <div class="row">
		<div class="col-md-4">
			<div class="row">
				<div class="col-md-4" style="margin: 10px;">
					First Number: <input type="number" name="first_num"
						ng-model="first_num">
				</div>
				<div class="col-md-4" style="margin: 10px;">
					Second Number: <input type="number" name="sec_num"
						ng-model="sec_num">
				</div>
				<div class="col-md-4" style="margin: 10px;">
				<select name="mySelect" id="mySelect"
				      ng-options="option.name for option in data.availableOptions track by option.value"
				      ng-model="data.selectedOption"></select>
		 	   </div> 
			</div>
		</div>
		<div>
				<div>
					<strong>Result:</strong> <br><code style="margin: 10px;font-size:20px;">{{ result }}</code>
				</div>
		</div>
	</div>

	<div class="col-md-12" style="margin: 10px;">
		<input type="submit" class="btn btn-success" ng-click="submit();"
			value="Calculate" />
	</div>
	<p> 
  </body>
</html>